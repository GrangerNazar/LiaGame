save/encryption_on_exports_only=true
text/autopauses={}
variables={}
layout/style_list=[]
layout/default_style="Default"
glossary/default_case_sensitive=true

[display]

window/size/mode=3

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogic/plugin.cfg", "res://addons/zylann.hterrain/plugin.cfg")

